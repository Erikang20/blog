<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Home</title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css" integrity="sha384-1q8mTJOASx8j1Au+a5WDVnPi2lkFfwwEAa8hDDdjZlpLegxhjVME1fgjWPGmkzs7" crossorigin="anonymous">
    <link href="/main.css" rel="stylesheet" type="text/css">

  </head>
  <body id="home">
      <header>

        <h1>Welcome</h1>

      </header>


    <table class="table table-hover">
      <thead>
        <th>Name</th>
        <th>userName</th>

      </thead>
      <tbody>
        <tr>
          <% user.forEach(function(user){ %>

          <td><a href="/users/<%=user.id%>"><%= user.name %>   </td>
          <td> <%= user.userName %>  </td>

        </tr>
        <% }) %>
    </tbody>
  </table>

  <form action="new">
    <button type="submit" class="btn btn-info" value="go to new.ejs">New User</button>

  </form>

  </body>
</html>
